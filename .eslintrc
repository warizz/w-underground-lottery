{
  extends: "airbnb",
  rules: {
    max-len: [2, 200],
    react/jsx-filename-extension: [1, { "extensions": [".js", ".jsx"] }],
    no-underscore-dangle: [2, { "allow": ["_id"] }],
    react/no-unused-prop-types: [2, { "skipShapeProps": true }],
  },
  env: {
    browser: true
  },
  "globals": {
    "describe": true,
    "it": true,
    "expect": true
  }
}
